{
  "name": "nestjs-prisma-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start --watch",
    "dev-swc": "nodemon",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "gen:prisma": "prisma generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build-swc": "swc ./src -s -d dist-swc",
    "typecheck": "tsc -p . --noEmit",
    "lint:eslint": "eslint . --ext ts --cache",
    "lint:prettier": "yarn run:prettier --check",
    "lint:cspell": "cspell lint './**/*.{ts,js,json,md,yml,yaml}' --gitignore --cache",
    "lint": "run-s 'lint:*'",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn run:prettier --write",
    "fix": "run-s 'fix:*'",
    "run:prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}'",
    "lint-staged": "lint-staged --allow-empty -r",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare:husky": "husky install",
    "prepare:git": "git config --local --add merge.ff false && git config --local --add pull.ff only",
    "prepare": "run-p 'prepare:*'"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^3.14.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.194",
    "@swc/helpers": "^0.3.16",
    "@tsconfig/strictest": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "commitizen": "^4.2.4",
    "cspell": "^6.0.0",
    "esbuild": "^0.14.41",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.2",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tsd": "^0.20.0",
    "typegraphql-prisma": "^0.20.3",
    "typescript": "^4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
